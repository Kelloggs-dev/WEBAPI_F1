<Window x:Class="WPF_WEBAPI_F1.P.C_CADRE_MODIF_CONSTRUCTEUR"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_WEBAPI_F1.P" xmlns:c="clr-namespace:WPF_WEBAPI_F1.C" d:DataContext="{d:DesignInstance Type=c:C_COORDINATION}"
        mc:Ignorable="d"
        Title="C_CADRE_MODIF_CONSTRUCTEUR" Height="222" Width="479">
    <Grid>
        <GroupBox Header="GroupBox" Margin="10,10,10,10">
            <Canvas>
                <Label Content="Constructeur :" Canvas.Left="54" Canvas.Top="47" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                <TextBox Canvas.Left="138" TextWrapping="Wrap" Text="{Binding Copie_Constructeur.Nom}" Canvas.Top="51" Width="167" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                <Button x:Name="BTN_Valider" Content="Valider" Canvas.Left="138" Canvas.Top="93" HorizontalAlignment="Left" VerticalAlignment="Center" Width="84" Click="BTN_Valider_Click"/>
                <Button x:Name="BTN_Annuler" Content="Annuler" Canvas.Left="227" Canvas.Top="93" HorizontalAlignment="Left" VerticalAlignment="Center" Width="78" Click="BTN_Annuler_Click"/>
            </Canvas>
        </GroupBox>

    </Grid>
</Window>
